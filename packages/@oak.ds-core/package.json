{
  "name": "@oak.ds/core",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "main": "index.js",
  "types": "index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c && pnpm ts-node-esm scripts/index post-build ",
    "dev": "pnpm build --watch",
