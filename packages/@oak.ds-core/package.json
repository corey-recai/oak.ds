  },
  "devDependencies": {
    "@oak.ds/eslint-config": "workspace:*",
    "@oak.ds/tsconfig": "workspace:*",
    "@rollup/plugin-typescript": "^11.1.0",
    "postcss": "^8.4.23",
    "rollup": "^3.21.0",
    "rollup-plugin-ignore-import": "^1.3.2",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-preset-solid": "^2.0.1",
    "solid-js": "^1.6.10",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
